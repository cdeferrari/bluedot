@model IList<Administracion.DomainModel.UnitAccountStatusSummary>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="page-content" class="sidebar-partial sidebar-visible-lg sidebar-no-animations">

    <div id="main-container">

        <div class="content-header">
            <ul class="nav-horizontal text-center">
                <li>
                    <a href="/DataBase/Index"><i class="fa fa-home"></i> Home</a>
                </li>

                <li class="active">
                    <a href="#"><i class="gi gi-building"></i> Consorcios</a>
                </li>
            </ul>
        </div>
        <ul class="breadcrumb breadcrumb-top">
            <li><a href="/home">Home</a></li>
            <li><a href="">Consorcio</a></li>
        </ul>
        <div class="block full">
            <div class="block-title">
                <h2><strong>Listado de</strong> Unidades</h2>
            </div>
            <p>Acá podrás encontrar el estado de cuenta de cada unidad</p>


            <div class="panel-heading">
                <div class="row">                    
                    <div class="col-md-2">                    
                    </div>
                </div>
            </div>



            <div class="table-responsive">
                @if (Model.Count() > 0)
                {

                    <table id="example-datatable" class="table table-vcenter table-condensed table-bordered reporte">                    

                            <thead>
                                <tr>
                                    <th class="text-center">Uf</th>
                                    <th class="text-center">Piso</th>
                                    <th class="text-center">Depto</th>
                                    <th class="text-center">Prop.</th>
                                    <th class="text-center">Saldo Anterior</th>
                                    <th class="text-center">Pagos</th>
                                    <th class="text-center">Deuda</th>
                                    <th class="text-center">Int</th>
                                    <th>Gastos A</th>
                                    <th>Gastos B</th>
                                    <th>Expensas</th>
                                    <th>Edesur</th>
                                    <th>Aysa</th>
                                    <th>Antes del</th>
                                    <th class="text-center">Total</th>
                                </tr>
                            </thead>

                            <tbody>
                                @foreach (var unitResume in Model)
                                {
                                    @Html.Partial("_UnitResumeItem", unitResume)
                                }
                            </tbody>
                        </table>

                }
                

            </div>
        </div>


    </div>
</div>
